    "def find_anagrams_bad(lst):\n",
    "    result = list()\n",
    "    for words in lst:\n",
    "        for word in words:\n",
    "            if words != word and sorted(word) == sorted(words):\n",
    "                if word not in result:\n",
    "                    result.append(word)\n",
    "                if words not in result:\n",
    "                    result.append(words)\n",
    "    return \n",
